Cell[BoxData[
 RowBox[{
  RowBox[{"nPnearSphere", "[", 
   RowBox[{"\[Lambda]_", ",", "nb_", ",", "ns_", ",", "r0_", ",", "dis_", ",", 
    RowBox[{"radiative_", ":", "0"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"num", "=", "50"}], ",", "k0", ",", "kb", ",", "ks", ",", 
      "kbr0", ",", "ksr0", ",", "kbr", ",", "a", ",", "b", ",", "DSphJ", ",", 
      "DSphH", ",", "pRad", ",", "pTange", ",", "pRadradiative", ",", "\n", 
      "pTangeradiative"}], "}"}], ",", 
    RowBox[{
     RowBox[{"k0", "=", 
      RowBox[{"N", "[", 
       FractionBox[
        RowBox[{"2", "*", "\[Pi]"}], "\[Lambda]"], "]"}]}], ";", 
     RowBox[{"kb", "=", 
      RowBox[{"k0", "*", "nb"}]}], ";", 
     RowBox[{"ks", "=", 
      RowBox[{"k0", "*", "ns"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"kbr0", "=", 
      RowBox[{"kb", "*", "r0"}]}], ";", 
     RowBox[{"ksr0", "=", 
      RowBox[{"ks", "*", "r0"}]}], ";", 
     RowBox[{"kbr", "=", 
      RowBox[{"kb", "*", "dis"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"The", " ", "derivative"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"DSphJ", "[", 
       RowBox[{"l_", ",", "x_"}], "]"}], ":=", 
      RowBox[{
       FractionBox[
        RowBox[{"SphericalBesselJ", "[", 
         RowBox[{"l", ",", "x"}], "]"}], "2"], "+", 
       RowBox[{
        FractionBox["x", "2"], "*", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"SphericalBesselJ", "[", 
           RowBox[{
            RowBox[{"l", "-", "1"}], ",", "x"}], "]"}], "-", 
          RowBox[{"SphericalBesselJ", "[", 
           RowBox[{
            RowBox[{"l", "+", "1"}], ",", "x"}], "]"}]}], ")"}]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"DSphH", "[", 
       RowBox[{"l_", ",", "x_"}], "]"}], ":=", 
      RowBox[{
       FractionBox[
        RowBox[{"SphericalHankelH1", "[", 
         RowBox[{"l", ",", "x"}], "]"}], "2"], "+", 
       RowBox[{
        FractionBox["x", "2"], "*", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"SphericalHankelH1", "[", 
           RowBox[{
            RowBox[{"l", "-", "1"}], ",", "x"}], "]"}], "-", 
          RowBox[{"SphericalHankelH1", "[", 
           RowBox[{
            RowBox[{"l", "+", "1"}], ",", "x"}], "]"}]}], ")"}]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"The", " ", "Mie", " ", "Scattering", " ", "Coefficients"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"b", "[", "l_", "]"}], ":=", 
      RowBox[{"N", "[", 
       RowBox[{"-", 
        FractionBox[
         RowBox[{
          RowBox[{
           RowBox[{"ns", "^", "2"}], "*", 
           RowBox[{"SphericalBesselJ", "[", 
            RowBox[{"l", ",", "ksr0"}], "]"}], "*", 
           RowBox[{"DSphJ", "[", 
            RowBox[{"l", ",", "kbr0"}], "]"}]}], "-", 
          RowBox[{
           RowBox[{"SphericalBesselJ", "[", 
            RowBox[{"l", ",", "kbr0"}], "]"}], "*", 
           RowBox[{"DSphJ", "[", 
            RowBox[{"l", ",", "ksr0"}], "]"}]}]}], 
         RowBox[{
          RowBox[{
           RowBox[{"ns", "^", "2"}], "*", 
           RowBox[{"SphericalBesselJ", "[", 
            RowBox[{"l", ",", "ksr0"}], "]"}], "*", 
           RowBox[{"DSphH", "[", 
            RowBox[{"l", ",", "kbr0"}], "]"}]}], "-", 
          RowBox[{
           RowBox[{"SphericalHankelH1", "[", 
            RowBox[{"l", ",", "kbr0"}], "]"}], "*", 
           RowBox[{"DSphJ", "[", 
            RowBox[{"l", ",", "ksr0"}], "]"}]}]}]]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"a", "[", "l_", "]"}], ":=", 
