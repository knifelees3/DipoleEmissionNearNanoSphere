     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "nm", ",", "\[Lambda]", ",", "nb", ",", "ns", ",", "\[Alpha]", ",", 
         "nPList", ",", "rvec", ",", "\[Beta]"}], "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"nm", "=", 
         RowBox[{"10", "^", 
          RowBox[{"(", 
           RowBox[{"-", "9"}], ")"}]}]}], ";", "\n", 
        RowBox[{"rvec", "=", 
         RowBox[{"postionSph", "[", 
          RowBox[{"1", ",", "\[Theta]", ",", "\[Phi]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"\[Alpha]", "=", 
         RowBox[{"rvec", ".", "p"}]}], ";", "\n", 
        RowBox[{"\[Beta]", "=", 
         SqrtBox[
          RowBox[{"1", "-", 
           SuperscriptBox["\[Alpha]", "2"]}]]}], ";", "\[IndentingNewLine]", 
        RowBox[{"nPList", "=", 
         RowBox[{"N", "[", 
          RowBox[{
           RowBox[{
            SuperscriptBox["\[Alpha]", "2"], "*", 
            RowBox[{"nP", "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], "+", 
           RowBox[{
            RowBox[{"\[Beta]", "^", "2"}], "*", 
            RowBox[{"nP", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}]}], "]"}]}]}]}], "]"}]}], ";"}], 
