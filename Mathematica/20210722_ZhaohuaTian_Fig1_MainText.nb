     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "rotFun", ",", "dx", ",", "dy", ",", "dz", ",", "norm", ",", "rotmat", 
        ",", "d1", ",", "d2", ",", "d3"}], "}"}], ",", "\n", 
      RowBox[{
       RowBox[{"rotmat", "=", 
        RowBox[{"N", "[", 
         RowBox[{
          RowBox[{"RotationMatrix", "[", 
           RowBox[{"\[Phi]z", ",", 
            RowBox[{"{", 
             RowBox[{"0", ",", "0", ",", "1"}], "}"}]}], "]"}], ".", 
          RowBox[{"RotationMatrix", "[", 
           RowBox[{"\[Phi]y", ",", 
            RowBox[{"{", 
             RowBox[{"0", ",", "1", ",", "0"}], "}"}]}], "]"}], ".", 
          RowBox[{"RotationMatrix", "[", 
           RowBox[{"\[Phi]x", ",", 
            RowBox[{"{", 
             RowBox[{"1", ",", "0", ",", "0"}], "}"}]}], "]"}]}], "]"}]}], 
       ";", "\n", "\n", 
       RowBox[{"dx", "=", 
        RowBox[{"N", "[", 
         RowBox[{"{", 
          RowBox[{"1", ",", "0", ",", "0"}], "}"}], "]"}]}], ";", "\n", 
       RowBox[{"dy", "=", 
        RowBox[{"N", "[", 
         RowBox[{"{", 
          RowBox[{"0", ",", "\[Alpha]", ",", "0"}], "}"}], "]"}]}], ";", "\n", 
       RowBox[{"dz", "=", 
        RowBox[{"N", "[", 
         RowBox[{"{", 
          RowBox[{"0", ",", "0", ",", "\[Beta]"}], "}"}], "]"}]}], ";", "\n", 
       "\n", 
       RowBox[{"norm", "=", 
        RowBox[{"N", "[", 
         SqrtBox[
          RowBox[{"1", "+", 
           RowBox[{"\[Alpha]", "^", "2"}], "+", 
           RowBox[{"\[Beta]", "^", "2"}]}]], "]"}]}], ";", "\n", 
       RowBox[{"d1", "=", 
        RowBox[{"N", "[", 
         RowBox[{
          RowBox[{"rotmat", ".", "dx"}], "/", "norm"}], "]"}]}], ";", "\n", 
       RowBox[{"d2", "=", 
        RowBox[{"N", "[", 
         RowBox[{
          RowBox[{"rotmat", ".", "dy"}], "/", "norm"}], "]"}]}], ";", "\n", 
       RowBox[{"d3", "=", 
        RowBox[{"N", "[", 
         RowBox[{
          RowBox[{"rotmat", ".", "dz"}], "/", "norm"}], "]"}]}], ";", "\n", 
       RowBox[{"Return", "[", 
        RowBox[{"{", 
         RowBox[{"d1", ",", "d2", ",", "d3"}], "}"}], "]"}]}]}], "]"}]}], 
   ";"}], "\n", "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"postionSph", "[", 
     RowBox[{"r_", ",", "\[Theta]_", ",", "\[Phi]_"}], "]"}], ":=", 
