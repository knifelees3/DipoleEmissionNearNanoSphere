          RowBox[{
           RowBox[{"SphericalBesselJ", "[", 
            RowBox[{"l", ",", "ksr0"}], "]"}], "*", 
           RowBox[{"DSphH", "[", 
            RowBox[{"l", ",", "kbr0"}], "]"}]}], "-", 
