           RowBox[{"SphericalHankelH1", "[", 
            RowBox[{"l", ",", "kbr0"}], "]"}], "*", 
           RowBox[{"DSphJ", "[", 
            RowBox[{"l", ",", "ksr0"}], "]"}]}]}]]}], "]"}]}], ";", 
